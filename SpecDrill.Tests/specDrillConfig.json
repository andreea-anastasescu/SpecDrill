      "drivers": {
        "chrome": {
          "path": "%USERPROFILE%\\.nuget\\packages\\Selenium.WebDriver.ChromeDriver\\96.0.4664.4500\\driver\\win32", //"C:\\Your Browser Drivers Path"//
