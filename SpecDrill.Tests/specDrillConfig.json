            //"headless",
            "--disable-extensions",
            "disable-features=VizDisplayCompositor",
            "--incognito",
            "--enable-javascript",
            "--disable-gpu",
            "--disable-dev-shm-usage",
            "--ignore-certificate-errors"
            //"enable-features=NetworkServiceInProcess",
