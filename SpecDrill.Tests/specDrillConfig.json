        },
        "opera": { "path": "" },
        "safari": { "path": "" }
      },
      "window": {
        "isMaximized": true, // if true, remaining browserWindow properties will be ignored
        "initialWidth": 640, // defaults to 800
        "initialHeight": 720 // defaults to 600
      },
      "screenshots": { "path": "D:\\" },
      "capabilities": {
        "x": "yy"
      }
    },
    "appium": { // settings for appium mode -> targets Appium server
